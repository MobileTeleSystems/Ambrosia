                "metadata": {},
                "source": [
                    "### Saving the transformation parameters to use them after the experiment is completed"
                ]
            },
            {
                "cell_type": "code",
                "execution_count": 8,
                "id": "3068c971",
                "metadata": {},
                "outputs": [],
                "source": [
                    "transformer.store_params('examples_configs/kion_cuped_params.json')"
                ]
